<div>
    <h3 class="thin"><%= players.ewo.blank? ? "Emen" : "Ewo" %></h3>
    <table class="table table-bordered table-hover table-nonfluid players">
      <thead>
      <tr>
        <th>Player</th>
        <th>Fantasy Points</th>
      </tr>
      </thead>
      <tbody>
      <% players.each do |player| %>
          <tr>
            <td><%= link_to(player.name, player_path(player)) %></td>
            <td><%= player.points %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
</div>